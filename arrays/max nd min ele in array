import java.util.*;

class Mnm{
  public static void main(String args[]){
    Scanner s=new Scanner(System.in);
    int n=s.nextInt();
    int[] a=new int[n];
    
    for(int i=0; i<a.length; i++){
      a[i]=s.nextInt();
    }

    int max=a[0], min=a[0];

    for(int i=0; i<a.length; i++){
      if(a[i]>max) max=a[i];
      if(a[i]<min) min=a[i];
    }
    System.out.println("max element of an array : "+max);
    System.out.println("min element of an array : "+min);

  }
}
    
