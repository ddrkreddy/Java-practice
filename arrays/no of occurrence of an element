import java.util.*;

class Noe{
  public static void main(String args[]){
    Scanner s=new Scanner(System.in);
    int n=s.nextInt();
    int[] a=new int[n];

    for(int i=0; i<a.length; i++){
      a[i]=s.nextInt();
    }

    int x=s.nextInt();
    int c=0;
    for(int i=0; i<a.length; i++){
      if(a[i]==x)
        c++;
    }
    System.out.println("no. of occurrences of "+x+" in given array is : "+c);
  }
}
