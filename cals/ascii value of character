import java.util.*;
public class Main
{
	public static void main(String[] args) {
		//System.out.println("Hello World");
		Scanner s=new Scanner(System.in);
		char ch=s.next().charAt(0);
		int d=ch;
		System.out.println("entered character: "+ch);
		System.out.println("corresponding value: "+d);
	}
}
