°F = (9/5 × °C) + 32.
°C = (5/9 x °F) - 32.

if celsius = 0 degree then, fahrenheit = 32 degree.


  import java.util.*;
  class Ctof{
    public static void main(String args[]){
      Scanner s=new Scanner(System.in);
      int c=s.nextInt();
      int f=(9/5 * c) +32;
      
      System.out.println(f);
      System.out.println(c);
    }
  }
