no of odds from 1 to n.

import java.util.*;
class non{
  public static void main(String args[]){
    Scanner s=new Scanner(System.in);
    int n=s.nextInt();
    int count=0;
    for(int i=1;i<=n;i++){
      if(i%2!=0) { count++; }
    }
    System.out.println("no of odd nums from 1 to "+n+" is : "+count);
  }
}
