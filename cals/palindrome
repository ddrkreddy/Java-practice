import java.util.*;
class Palin{
  public static void main(String args[]){
    Scanner s=new Scanner(System.in);
    int n=s.nextInt();
    int rev=0,d;
    int temp=n;
    while(n>0){
      d=n%10;
      rev=rev*10 + d;
      n=n/10;
    }
    if(temp==rev) System.out.println(temp+" is a palindrome number.");
    else System.out.println(temp+" is not a palindrome number.");
  }
}
      
