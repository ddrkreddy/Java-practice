import java.util.*;
class Prime{
  public static void main(String args[]){
    Scanner s=new Scanner(System.in);
    int n=s.nextInt();
    boolean f=false;
    for(int i=2;i<=n/2;i++){
      if(n%i==0) f=true;
    }
    if(!f) System.out.println(n+" is a prime number.");
    else System.out.println(n+" is not a prime number.");
  
